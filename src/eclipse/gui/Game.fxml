            fx:controller="eclipse.gui.GameController">
    <stylesheets>
        <URL value="@Style.css"/>
    </stylesheets>
    <children>
        <Pane fx:id="gameArea" layoutX="1.0" layoutY="2.0" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Label fx:id="scoreLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="149.0" prefHeight="37.0"
