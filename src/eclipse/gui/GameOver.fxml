<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="600.0" prefWidth="450.0" style="-fx-background-color: black;" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eclipse.gui.GameOverController">
   <children>
      <VBox fx:id="box" alignment="CENTER" prefHeight="600.0" prefWidth="450.0" spacing="15.0">
         <children>
            <Label style="-fx-font-size: 80; -fx-text-fill: red;" text="Game Over">
               <VBox.margin>
                  <Insets bottom="60.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" prefWidth="450.0" spacing="25.0">
               <children>
                  <Button fx:id="start" mnemonicParsing="false" text="Play Again" />
                  <Button fx:id="home" mnemonicParsing="false" text="Main Menu">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="90.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
